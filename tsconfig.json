{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Node",
        "strict": true,
        "jsx": "preserve",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "types": ["vite/client"],
        "baseUrl": ".",
        "paths": {
              "@/*": ["src/*"],
        },
    },
    "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.vue"]
}
